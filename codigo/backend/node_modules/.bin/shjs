#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules/shelljs/bin/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules/shelljs/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/node_modules:/home/marcelo/dev/Modulo6-Grupo5/node_modules:/home/marcelo/dev/node_modules:/home/marcelo/node_modules:/home/node_modules:/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules/shelljs/bin/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules/shelljs/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/node_modules:/home/marcelo/dev/Modulo6-Grupo5/node_modules:/home/marcelo/dev/node_modules:/home/marcelo/node_modules:/home/node_modules:/node_modules:/home/marcelo/dev/Modulo6-Grupo5/codigo/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shelljs/bin/shjs" "$@"
else
  exec node  "$basedir/../shelljs/bin/shjs" "$@"
fi
